{
  "name": "@bamf-health/bamfjs",
  "title": "bamfjs",
  "description": "A bunch of JavaScript functions that we may end up using in various projects.",
  "version": "1.0.0",
  "author": "Karl Swedberg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint -c .eslintrc.js {rollup.config.js,test/,src/}",
    "docs": "node ./scripts/build-docs.js && rm ./scripts/api.md",
    "build:es": "NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:cjs": "NODE_ENV=production rollup --config rollup.config.js --format cjs",
    "build:scripts": "npm run lint && NODE_ENV=production rollup --config rollup.config.js",
    "build": "npm run lint && NODE_ENV=production rollup --config rollup.config.js && npm run docs",
    "test:mocha": "TEST_ENV=node mocha --timeout 5000 --require jsdom-global/register --require mock-local-storage test/mocha",
    "test": "npm run build:scripts && npm run test:mocha",
    "watch": "node ./scripts/watch.js",
    "serve": "nodemon --watch test/index.html --watch test/qunit ./scripts/server.js",
    "prepublishOnly": "npm run build:es && npm run build:cjs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bamf-health/bamfjs.git"
  },
  "type": "commonjs",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-strip": "^1.3.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-kswedberg": "^4.0.0",
    "fs-extra": "^8.1.0",
    "jsdoc-to-markdown": "^5.0.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "minimist": "^1.2.0",
    "mocha": "^3.5.3",
    "mock-local-storage": "^1.0.5",
    "nodemon": "^2.0.2",
    "qunitjs": "^2.4.0",
    "request": "^2.88.0",
    "rollup": "^1.29.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^5.1.3",
    "through2": "^3.0.1"
  }
}
