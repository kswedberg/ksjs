{
  "name": "@bamf-health/bamfjs",
  "title": "bamfjs",
  "description": "A bunch of JavaScript functions that we may end up using in various projects.",
  "version": "1.18.2",
  "author": "Karl Swedberg",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.js {rollup.config.mjs,test/,src/}",
    "docs": "node ./scripts/build-docs.mjs && rm ./scripts/api.md",
    "build:es": "NODE_ENV=production rollup --config rollup.config.mjs --format es",
    "build:cjs": "NODE_ENV=production rollup --config rollup.config.mjs --format cjs",
    "build:scripts": "npm run lint && NODE_ENV=production rollup --config rollup.config.mjs",
    "build": "npm run lint && NODE_ENV=production rollup --config rollup.config.mjs && npm run docs",
    "test:mocha": "TEST_ENV=node mocha --timeout 5000 --require jsdom-global/register --require mock-local-storage test/mocha",
    "test": "npm run build:scripts && npm run test:mocha",
    "watch": "node ./scripts/watch.mjs",
    "serve": "nodemon --watch test/index.html --watch test/qunit ./scripts/server.mjs",
    "prepublishOnly": "npm run build:es && npm run build:cjs && node ./scripts/cp.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bamf-health/bamfjs.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./cjs/index.js",
  "module": "./index.js",
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-eslint": "^9.0.4",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-strip": "^3.0.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-kswedberg": "^5.3.4",
    "fs-extra": "^11.1.1",
    "jsdoc-to-markdown": "^8.0.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "markdown-it": "^13.0.1",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "mock-local-storage": "^1.1.24",
    "nodemon": "^3.0.1",
    "qunit": "^2.19.4",
    "rollup": "^3.26.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "through2": "^4.0.2"
  }
}
